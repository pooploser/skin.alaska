<!--Alaska-->
<window id="34">
	<defaultcontrol always="true">4</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<include>Global_Background</include>
		<include>Global_Filter</include>
		<control type="group">
			<control type="group">
				<include>Animation_ZoomOutZoomIn</include>
				<control type="image">
					<description>Panel</description>
					<posx>192</posx>
					<posy>94</posy>
					<width>896</width>
					<height>504</height>
					<texture border="100">common/common_panel_3.png</texture>
					<colordiffuse>PanelDiffuse</colordiffuse>
				</control>
				<control type="image">
					<posx>237</posx>
					<posy>504</posy>
					<width>805</width>
					<height>2</height>
					<texture>common/divider.png</texture>
					<colordiffuse>PanelDiffuseDivider</colordiffuse>
				</control>
				<!--<control type="image">
					<posx>630</posx>
					<posy>128</posy>
					<height>20</height>
					<width>20</width>
					<texture>arrowupfo.png</texture>
					<colordiffuse>ButtonFilterColor</colordiffuse>
					<visible>Container(2).HasPrevious</visible>
				</control>
				<control type="image">
					<posx>630</posx>
					<posy>480</posy>
					<height>20</height>
					<width>20</width>
					<texture>arrowdownfo.png</texture>
					<colordiffuse>ButtonFilterColor</colordiffuse>
					<visible>Container(2).HasNext</visible>
				</control>-->
				<control type="radiobutton" id="4">
					<description>Enable Login Screen</description>
					<posx>494</posx>
					<posy>316</posy>
					<width>700</width>
					<height>46</height>
					<align>right</align>
					<aligny>center</aligny>
					<radioposx>1000</radioposx>
					<radioposy>523</radioposy>
					<radiowidth>32</radiowidth>
					<radioheight>32</radioheight>
					<onright>2</onright>
					<onleft>2</onleft>
					<onup>4</onup>
					<ondown>4</ondown>
				</control>
				<control type="label" id="4">
					<description>Enable Login Screen</description>
					<posx>992</posx>
					<posy>516</posy>
					<width>700</width>
					<height>46</height>
					<align>right</align>
					<aligny>center</aligny>
					<radioposx>706</radioposx>
					<radioposy>8</radioposy>
					<radiowidth>32</radiowidth>
					<radioheight>32</radioheight>
					<textcolor>PanelListItemFocused</textcolor>
					<font>Font_PanelLabel</font>
					<label>$LOCALIZE[20096]</label>
					<visible>!Control.HasFocus(2)</visible>
				</control>
				<control type="label" id="4">
					<description>Enable Login Screen</description>
					<posx>992</posx>
					<posy>516</posy>
					<width>700</width>
					<height>46</height>
					<align>right</align>
					<aligny>center</aligny>
					<radioposx>706</radioposx>
					<radioposy>8</radioposy>
					<radiowidth>32</radiowidth>
					<radioheight>32</radioheight>
					<textcolor>PanelListItem</textcolor>
					<font>Font_PanelLabel</font>
					<label>$LOCALIZE[20096]</label>
					<visible>Control.HasFocus(2)</visible>
				</control>
				<control type="list" id="2">
					<posx>260</posx>
					<posy>140</posy>
					<width>781</width>
					<height>360</height>
					<onleft>4</onleft>
					<onright>4</onright>
					<onup>2</onup>
					<ondown>2</ondown>
					<itemlayout width="885" height="40">
						<control type="label">
							<posx>18</posx>
							<posy>12</posy>
							<width>830</width>
							<height>25</height>
							<aligny>center</aligny>
							<font>Font_PanelItemLabel</font>
							<textcolor>PanelListItem</textcolor>
							<selectedcolor>MenuValue</selectedcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</itemlayout>
					<focusedlayout width="828" height="104">
						<control type="image">
							<posx>0</posx>
							<posy>0</posy>
							<width>104</width>
							<height>104</height>
							<texture>list/listframe.png</texture>
							<animation type="Focus" reversible="false">
								<effect type="zoom" center="52,52" start="50" end="100" time="200" tween="cubic" easing="out" />
								<effect type="fade" start="40" end="100" time="200" />
							</animation>
						</control>
						<control type="image">
							<posx>12</posx>
							<posy>12</posy>
							<width>80</width>
							<height>80</height>
							<texture fallback="unknown-user.png" diffuse="list/listmatte.png">$INFO[ListItem.Icon]</texture>
							<aspectratio scalediffuse="false">stretch</aspectratio>
							<animation type="Focus" reversible="false">
								<effect type="zoom" center="52,52" start="50" end="100" time="200" tween="cubic" easing="out" />
								<effect type="fade" start="40" end="100" time="200" />
							</animation>
						</control>
						<control type="label">
							<animation effect="slide" end="0,14" condition="IsEmpty(ListItem.Label2)">Conditional</animation>
							<posx>120</posx>
							<posy>24</posy>
							<width>830</width>
							<height>25</height>
							<aligny>center</aligny>
							<font>Font_ListTitleBig</font>
							<textcolor>MenuValueFO</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
						<control type="label">
							<posx>120</posx>
							<posy>54</posy>
							<width>830</width>
							<height>25</height>
							<aligny>center</aligny>
							<font>Font_ListTitle</font>
							<textcolor>MenuValueFO</textcolor>
							<label>$INFO[ListItem.Label2]</label>
						</control>
					</focusedlayout>
				</control>
			</control>
		</control>
		<include>Global_ContextFilter</include>
		<include>Global_Time</include>
		<include>Setting_Heading</include>
		<include>Global_RSS</include>
		<include>Object_DebugGrid</include>
		<control type="group">
			<include>Debug</include>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>
	</controls>
</window>