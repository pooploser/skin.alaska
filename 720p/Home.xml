<!--Alaska-->

<window id="0">
	<allowoverlay>yes</allowoverlay>
	<controls>
		<include>Global_Filter</include>
		<control type="image">
			<posx>0</posx>
			<posy>250</posy>
			<width>1280</width>
			<height>120</height>
			<texture>common/white.png</texture>
			<colordiffuse>FocusLinesBackground</colordiffuse>
		</control>
		<control type="image">
			<posx>0</posx>
			<posy>250</posy>
			<width>1280</width>
			<height>2</height>
			<texture>common/line_1.png</texture>
			<colordiffuse>55ffffff</colordiffuse>
		</control>
		<control type="image">
			<posx>0</posx>
			<posy>330</posy>
			<width>1280</width>
			<height>2</height>
			<texture>common/line_1.png</texture>
			<colordiffuse>55ffffff</colordiffuse>
		</control>
		<control type="image">
			<posx>0</posx>
			<posy>370</posy>
			<width>1280</width>
			<height>2</height>
			<texture>common/line_1.png</texture>
			<colordiffuse>55ffffff</colordiffuse>
		</control>
		<control type="image">
			<posx>10</posx>
			<posy>10</posy>
			<width>50</width>
			<height>50</height>
			<texture>shutdown.png</texture>
			<colordiffuse>88ADDFFF</colordiffuse>
		</control>
		<control type="image">
			<posx>16</posx>
			<posy>15</posy>
			<width>39</width>
			<height>39</height>
			<visible>Control.HasFocus(99120)</visible>
			<texture diffuse="setting_mask.png">black.png</texture>
			<colordiffuse>44000000</colordiffuse>
		</control>
		<control type="image">
			<posx>51</posx>
			<posy>11</posy>
			<width>50</width>
			<height>50</height>
			<texture>settings.png</texture>
			<colordiffuse>77ffffff</colordiffuse>
		</control>
		<control type="image">
			<posx>57</posx>
			<posy>15</posy>
			<width>39</width>
			<height>39</height>
			<visible>Control.HasFocus(99121)</visible>
			<texture diffuse="setting_mask.png">black.png</texture>
			<colordiffuse>44000000</colordiffuse>
		</control>
		<control type="image">
			<posx>93</posx>
			<posy>11</posy>
			<width>48</width>
			<height>48</height>
			<texture>search.png</texture>
			<colordiffuse>88ffffff</colordiffuse>
		</control>
		<control type="image">
			<posx>98</posx>
			<posy>15</posy>
			<width>41</width>
			<height>41</height>
			<visible>Control.HasFocus(99122)</visible>
			<texture diffuse="setting_mask.png">black.png</texture>
			<colordiffuse>44000000</colordiffuse>
		</control>
		<control type="button" id="99120">
			<posx>15</posx>
			<posy>15</posy>
			<width>39</width>
			<height>39</height>
			<include condition="!Skin.HasSetting(sleepsystem)">type_quit</include>
			<include condition="Skin.HasSetting(sleepsystem)">type_sleep</include>
			<ondown>300</ondown>
			<onright>99121</onright>
		</control>
		<control type="button" id="99121">
			<posx>57</posx>
			<posy>15</posy>
			<width>39</width>
			<height>39</height>
			<onclick>ActivateWindow(4)</onclick>
			<ondown>300</ondown>
			<onright>99122</onright>
			<onleft>99120</onleft>
		</control>
		<control type="button" id="99122">
			<posx>98</posx>
			<posy>15</posy>
			<width>41</width>
			<height>41</height>
			<onclick>ActivateWindow(10051)</onclick>
			<ondown>300</ondown>
			<onright>300</onright>
			<onleft>99121</onleft>
		</control>
		<include condition="[Skin.HasSetting(home1) | [!Skin.HasSetting(home1) + !Skin.HasSetting(home2) + !Skin.HasSetting(home3) + !Skin.HasSetting(home4)]] + !Skin.HasSetting(stackmedia)">Home_MenuImage</include>
		<include condition="[Skin.HasSetting(home1) | [!Skin.HasSetting(home1) + !Skin.HasSetting(home2) + !Skin.HasSetting(home3) + !Skin.HasSetting(home4)]] + Skin.HasSetting(stackmedia)">Home_MenuImage_stacked</include>
		<include condition="Skin.HasSetting(home2)">Home_MenuIcon</include>
		<include condition="Skin.HasSetting(home3)">Home_MenuList</include>
		<include condition="Skin.HasSetting(home4)">Home_MenuBlur</include>

		<control type="label">
			<include>WindowID</include>
			<label>Home.xml</label>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>
		<include>Global_ContextFilter</include>
		<include>New_lists</include>
		<include>Global_Time_Home</include>
		<include>Global_RSS</include>
		<include>Object_DebugGrid</include>
	</controls>
	<include condition="[Skin.HasSetting(home1) + Skin.HasSetting(stackmedia)] | Skin.HasSetting(home4) | [!Skin.HasSetting(home1) + !Skin.HasSetting(home2) + !Skin.HasSetting(home3) + !Skin.HasSetting(home4) + Skin.HasSetting(stackmedia)]">home4defaultcontrol</include>
	<include condition="Skin.HasSetting(home2) | Skin.HasSetting(home3) | [Skin.HasSetting(home1) + !Skin.HasSetting(stackmedia)] | [!Skin.HasSetting(home1) + !Skin.HasSetting(home2) + !Skin.HasSetting(home3) + !Skin.HasSetting(home4) + !Skin.HasSetting(stackmedia)]">homedefaultcontrol</include>

</window>