<!--Alaska-->

<window id="145">
	<defaultcontrol>11</defaultcontrol>
	<animation type="WindowOpen" reversible="false">
		<effect type="zoom" start="80" end="100" center="auto" delay="200" easing="out" tween="back" time="300" />
		<effect type="fade" delay="200" start="0" end="100" time="300" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="zoom" start="100" end="80" center="auto" easing="in" tween="back" time="300" />
		<effect type="fade" start="100" end="0" time="300" />
	</animation>
	<controls>

		<control type="label">
			<include>DialogID</include>
			<label>DialogSlider.xml</label>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>

		<control type="group">
			<posx>880</posx>
			<posy>90</posy>
			<width>707</width>
			<height>183</height>

			<control type="image">
				<posx>322</posx>
				<posy>10</posy>
				<width>50</width>
				<height>50</height>
				<texture>dialogs/slider_back.png</texture>
			</control>
			<control type="slider" id="11">
				<description>Slider</description>
				<posx>328</posx>
				<posy>27</posy>
				<width>38</width>
				<height>13</height>
				<texturesliderbar>osd/sliderback.png</texturesliderbar>
				<textureslidernib>osdslidernibnf.png</textureslidernib>
				<textureslidernibfocus>osdslidernibfo.png</textureslidernibfocus>
				<animation type="WindowOpen" reversible="false">
					<effect type="fade" delay="400" start="0" end="100" time="100" />
				</animation>
				<animation type="WindowClose" reversible="false">
					<effect type="fade" start="100" end="0" time="10" />
				</animation>
				<onup>11</onup>
				<ondown>11</ondown>
				<onleft>11</onleft>
				<onright>11</onright>
			</control>
			<control type="image">
				<posx>322</posx>
				<posy>10</posy>
				<width>50</width>
				<height>50</height>
				<texture>dialogs/slider.png</texture>
			</control>

			<control type="label" id="10">
				<description>Heading Label</description>
				<posx>310</posx>
				<posy>15</posy>
				<width>627</width>
				<height>51</height>
				<textcolor>Time</textcolor>
				<align>right</align>
				<font>Font_ProgressDialog</font>
			</control>

			<control type="label" id="12">
				<posx>310</posx>
				<posy>35</posy>
				<width>627</width>
				<height>51</height>
				<textcolor>Date</textcolor>
				<align>right</align>
				<font>Font_Date</font>
			</control>
		</control>

		<include>Object_DebugGrid</include>

	</controls>
</window>